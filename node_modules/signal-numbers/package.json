{
  "name": "signal-numbers",
  "version": "0.2.2",
  "description": "Converts signal strings to numbers.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "lint": "tslint -c tslint.json src/**/*.ts && tslint -c tests/tslint.json tests/**/*.ts",
    "mocha": "nyc mocha -r ts-node/register tests/**/*.ts --recursive",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "start": "node lib/index.js",
    "test": "NODE_ENV=test npm run lint && NODE_ENV=test npm run mocha",
    "test-only": "npm run lint && mocha -r ts-node/register tests/**/*.ts --recursive",
    "watch": "nodemon --ext ts --ignore lib --watch src --watch tests --exec 'npm run build && npm run test-only || true' --delay 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluk/signal-numbers.git"
  },
  "keywords": [
    "signal",
    "signals"
  ],
  "author": "Bryant Luk",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^13.2.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "watchify": "^3.11.0"
  },
  "bugs": {
    "url": "https://github.com/bluk/signal-numbers/issues"
  },
  "homepage": "https://github.com/bluk/signal-numbers#readme"
}
